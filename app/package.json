{
  "scripts": {
    "check-types": "tsc",
    "start": "npm run serve:dev",
    "build:staging": "cross-env NODE_ENV=staging webpack --color --progress",
    "build:production": "cross-env NODE_ENV=production webpack --color --progress",
    "serve:dev": "cross-env NODE_ENV=development webpack-dev-server --color --progress",
    "serve:staging": "cross-env NODE_ENV=staging webpack-dev-server --color --progress",
    "serve:production": "cross-env NODE_ENV=production webpack-dev-server --color --progress",
    "serve:dist": "cd credentials; http-server ../dist -S -C cert.pem",
    "predeploystaging": "npm run build:staging",
    "predeployproduction": "npm run build:production",
    "deploystaging": "aws s3 sync ./dist s3://staging.liquid-vote.com --profile pedro",
    "deployproduction": "aws s3 sync ./dist s3://app.liquid-vote.com --profile pedro",
    "emptyS3": "aws s3 rm s3://app.liquid-vote.com --recursive --profile pedro",
    "aws:install": "curl 'https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip' -o 'awscliv2.zip'; unzip awscliv2.zip; sudo ./aws/install",
    "aws:login": "aws configure import --csv file://./credentials/pedro_accessKeys.csv"
  },
