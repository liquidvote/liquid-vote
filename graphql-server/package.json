{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index-server.ts",
    "dev:server": "npx sls offline",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "npx sls deploy",
    "deploy:live": "npx sls deploy --stage live",
    "remove:dev": "npx sls remove --stage dev",
    "remove:live": "npx sls remove --stage live",
    "logs": "npx sls logs -f graphql -t -s dev",
    "demo:emails": "ejs src/state/Notifications/email-template.ejs -f src/state/Notifications/email-template.json -o debug/emails.html",
    "db:dumpLiveToLocal": "node migrations/dumpLiveToLocal.js"
  },
