    "@aws-sdk/types": "^3.29.0",
    "@sentry/serverless": "^7.12.1",
    "apollo-datasource-rest": "^3.2.0",
    "apollo-server-lambda": "^2",
    "aws-sdk": "^2.1184.0",
    "ejs": "^3.1.8",
    "firebase-admin": "^11.0.0",
    "graphql": "^15.6.1",
    "mongodb": "^3.7"
  },
  "devDependencies": {
    "apollo-server": "^2",
    "nodemon": "^2.0.14",
